{
    "name": "deesoft/yii2-client-url",
    "description": "Yii2 js url generator",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","route"],
    "license": "MIT",
    "authors": [
        {
            "name": "mdmunir",
            "email": "misbahuldmunir@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "dee\\clientUrl\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
